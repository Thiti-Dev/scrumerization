#/* ------------------------------ Custom Typing ----------------------------- */

scalar UUID
scalar Time

#/* -------------------------------------------------------------------------- */

#/* ---------------------------------- ENUMS --------------------------------- */
enum Role {
    ADMIN
    USER
}
#/* -------------------------------------------------------------------------- */

#/* ------------------------------- Directives ------------------------------- */
directive @requiredMember(role: Role!) on FIELD_DEFINITION

directive @jsonConstraint(
	format: String
) on INPUT_FIELD_DEFINITION | FIELD_DEFINITION

#/* -------------------------------------------------------------------------- */

type Query {
    roles: [Role!]!
}

type Mutation {
    blankMutation: Boolean!
}